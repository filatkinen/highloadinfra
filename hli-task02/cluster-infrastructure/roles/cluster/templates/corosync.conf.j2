totem {
  version: 2
  secauth: on
  threads: 0
  interface {
    ringnumber: 0
    bindnetaddr: 192.168.50.0
    mcastaddr: 239.255.1.1
    mcastport: 5405
  }
}

logging {
  fileline: off
  to_stderr: yes
  to_logfile: no
  to_syslog: yes
  syslog_facility: daemon
}

nodelist {
  node {
    ring0_addr: cluster-node-1
    nodeid: 1
  }
  node {
    ring0_addr: cluster-node-2
    nodeid: 2
  }
  node {
    ring0_addr: cluster-node-3
    nodeid: 3
  }
}

quorum {
  provider: corosync_votequorum
}
